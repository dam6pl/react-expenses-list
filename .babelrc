{
    "env": {
        "production": {
            "plugins": [
                "emotion"
            ]
        },
        "development": {
            "plugins": [
                [
                    "emotion",
                    {
                        "sourceMap": true
                    }
                ],
                "transform-decorators-legacy",
                [
                    "transform-class-properties",
                    {
                        "spec": true
                    }
                ]
            ]
        }
    }
}